{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "cache": true,
      "dependsOn": ["^build", "clean"]
    },
    "clean": {},
    "test": {},
    "lint": {
      "cache": true,
      "dependsOn": ["^lint"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "cache": true
    }
  }
}
