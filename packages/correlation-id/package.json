{
  "name": "@nestified/correlation-id",
  "version": "1.0.0-dev.8",
  "private": false,
  "description": "A NestJS package for adding correlation IDs to requests for tracking and logging",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "prerelease": "pnpm version prerelease --preid=dev",
    "publish": "pnpm publish --access public --no-git-checks",
    "publish:dev": "pnpm publish --tag dev --access public --no-git-checks",
    "test": "vitest",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nestjs",
    "correlation-id",
    "request-tracking",
    "logging"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soltanireza65/nestified.git",
    "directory": "packages/correlation-id"
  },
  "homepage": "https://github.com/soltanireza65/nestified#readme",
  "bugs": {
    "url": "https://github.com/soltanireza65/nestified/issues"
  },
  "author": "Reza Soltani",
  "license": "MIT",
  "devDependencies": {
    "@nestjs/testing": "^11.1.6",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^10.0.0",
    "rxjs": "^7.8.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
