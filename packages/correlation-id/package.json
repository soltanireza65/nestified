{
  "name": "@nestified/correlation-id",
  "version": "1.0.0",
  "private": false,
  "description": "A NestJS package for adding correlation IDs to requests for tracking and logging",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "publish": "npm publish --access public",
    "test": "vitest",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nestjs",
    "correlation-id",
    "request-tracking",
    "logging"
  ],
  "author": "Reza Soltani",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/microservices": "^11.1.6",
    "@types/express": "^5.0.3",
    "express": "^5.1.0",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.6",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@nestjs/core": "^10.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
