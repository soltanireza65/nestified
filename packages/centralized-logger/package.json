{
  "name": "@nestified/centralized-logger",
  "version": "1.0.0-dev.2",
  "private": false,
  "description": "A structured logging module for NestJS based on Winston with correlation IDs, redaction, and request interceptors.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "prerelease": "pnpm version prerelease --preid=dev",
    "publish": "pnpm publish --access public --no-git-checks",
    "publish:dev": "pnpm publish --tag dev --access public --no-git-checks",
    "test": "vitest",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nestjs",
    "logger",
    "winston",
    "structured-logging",
    "correlation-id",
    "redaction",
    "logging"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soltanireza65/nestified.git",
    "directory": "packages/centralized-logger"
  },
  "homepage": "https://github.com/soltanireza65/nestified#readme",
  "bugs": {
    "url": "https://github.com/soltanireza65/nestified/issues"
  },
  "author": "Reza Soltani",
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0"
  },
  "dependencies": {
    "@nestified/correlation-id": "*",
    "logform": "^2.7.0",
    "rxjs": "^7.8.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.6",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
